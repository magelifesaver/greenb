{
    "name": "cyclonecode/custom-php-settings",
    "type": "wordpress-plugin",
    "keywords": [
        "wordpress",
        "plugin",
        "development",
        ".htaccess",
        "apache",
        "apache2",
        "php",
        "settings",
        "php settings",
        "environment variable"
    ],
    "description": "Customize php settings for you wordpress installation.",
    "suggest": {
        "ext-json": "*",
        "ext-yaml": "*"
    },
    "require": {
        "php": ">=5.6",
        "freemius/wordpress-sdk": "^2.13"
    },
    "require-dev": {
        "composer/installers": "^1.1",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "license": "GPL-2.0-or-later",
    "readme": "readme.txt",
    "homepage": "https://wordpress.org/plugins/custom-php-settings/",
    "authors": [
        {
            "name": "Cyclonecode",
            "email": "cyclonecode@gmail.com",
            "role": "Maintainer"
        }
    ],
    "support": {
        "email": "cyclonecode.help@gmail.com",
        "issues": "https://wordpress.org/support/plugin/custom-php-settings/",
        "source": "https://plugins.trac.wordpress.org/browser/custom-php-settings/",
        "docs": "https://wordpress.org/plugins/custom-php-settings/#description",
        "rss": "https://plugins.trac.wordpress.org/log/custom-php-settings/?limit=100&mode=stop_on_copy&format=rss",
        "chat": "https://join.slack.com/t/cyclonecode/shared_invite/zt-6bdtbdab-n9QaMLM~exHP19zFDPN~AQ"
    },
    "autoload": {
        "psr-4": {
            "CustomPhpSettings\\": [
                "src/"
            ]
        }
    },
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true,
        "preferred-install": "dist",
        "prepend-autoloader": false,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
